@{
    Layout = "~/Views/Shared/_Mobile.cshtml";
}
@{
    ViewBag.Title = @ViewData["title"].ToString();
}
@*返回菜单*@
@Html.Partial("BackBanner", new List<string> { "开始全真模拟测试", CommonLib.Web.WebTool.CDN("/exam/menu") })



<!--习题插入的div，id最好不要弄错-->
<div class="km-mnks-wp">
    <table class="km-mnks-tb">
        <tr>
            <td class="col1">考题数量</td>
            <td><span class="gold">@CommonLib.Common.ConfigReader.Read("Examcount")</span>题</td>
        </tr>
        <tr>
            <td class="col1">测试时间</td>
            <td><span class="gold">@CommonLib.Common.ConfigReader.Read("Examtime")</span>分钟</td>
        </tr>
        <tr>
            <td class="col1">合格标准</td>
            <td>
                满分<span class="gold">@CommonLib.Common.ConfigReader.Read("Examcount")</span>分，<span class="gold">@CommonLib.Common.ConfigReader.Read("Passscore", BaseLib.SystemType.Web)</span>分及格
            </td>
        </tr>
        <tr>
            <td class="col1">出题规则</td>
            <td>按公安部规定比例随机抽取</td>
        </tr>
    </table>
</div>

<a href="/exam/simulate/start" class="fullbtn">开始全真模拟测试</a>
<div class="bt-distance"></div>
