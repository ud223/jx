            <div class="li">年龄：@ViewData["age"]</div>
            <div class="li">报名时间：@(Convert.ToDateTime(((System.Collections.Hashtable)ViewData["stu"])["EnterDate"]).ToString("yyyy年MM月dd日")</div>
