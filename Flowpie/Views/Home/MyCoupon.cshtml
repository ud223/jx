@{
    Layout = "~/Views/Shared/_Mobile.cshtml";
}
@{
    ViewBag.Title = ViewData["title"];
}

@*返回菜单*@
@Html.Partial("BackBanner", new List<string> { "我的优惠券", CommonLib.Web.WebTool.CDN("/my/index") })


