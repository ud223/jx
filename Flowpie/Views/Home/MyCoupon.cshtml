<div class="jiluitms">
    @foreach (var item in ViewData["data"] as List<System.Collections.Hashtable>)
    {
        if (item["IsUse"].ToString() == "0")
        {
            <div class="jiluitm bb un-use-300q">
                <div class="msg">@item["CouponText"].ToString()</div>
                <div class="time"><span class="wmp">可使用</span></div>
                <div class="jxqmio"><sup>￥</sup> <span>@item["Amount"].ToString()</span> <sub>元</sub></div>
                <span>@item["Password"].ToString()</span>
            </div>
        }
        else
        {
            <div class="jiluitm bb is-use">
                <div class="msg">@item["CouponText"].ToString()</div>
                <div class="time"><span class="wmp">已使用</span></div>
                <div class="jxqmio"><sup>￥</sup> <span>@item["Amount"].ToString()</span> <sub>元</sub></div>
                <span>@item["Password"].ToString()</span>
            </div>
        }
    }
    <style>
        .aidi {
            background:#d2ffda;
        }
    </style>
    <div class="jiluitm bb aidi">
        <div class="msg">98元学车体验券</div>
        <div class="time"><span class="wmp">可使用</span></div>
        <div class="jxqmio"><sup>￥</sup> <span>98</span> <sub>元</sub></div>
    </div>
